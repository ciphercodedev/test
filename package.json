{
   "name": "minimals-next-simple",
   "version": "2.1.0",
   "private": true,
   "scripts": {
      "lint": "eslint --fix \"{pages,template,tools}/**/*.{tsx, ts, jsx, js}\"",
      "format": "prettier --write \"./pages/**/*.{tsx, ts, jsx, js}\"  \"template/**/*.{tsx, ts, jsx, js}\" \"tools/**/*.{tsx, ts, jsx, js}\"",
      "gql:schema": "apollo client:download-schema --endpoint=http://localhost:3000/graphql schema.graphql",
      "gql:codegen": "apollo client:codegen --no-addTypename --outputFlat --tagName=gql --target=typescript --localSchemaFile=schema.graphql --includes=**/*.gql.ts --excludes=tools/lib/apollo/apolloTypes.ts tools/lib/apollo/apolloTypes.ts",
      "gql": "npm run gql:schema && npm run gql:codegen",
      "dev": "npm run lint && npm run format && next dev -p 3222",
      "build": "next build",
      "start": "next start"
   },
   "dependencies": {
      "@apollo/client": "^3.3.20",
      "@emotion/cache": "^11.4.0",
      "@emotion/react": "^11.4.0",
      "@emotion/styled": "^11.3.0",
      "@hookform/resolvers": "^2.5.2",
      "@iconify/icons-eva": "^1.1.0",
      "@iconify/icons-ic": "^1.1.5",
      "@iconify/react": "^1.1.4",
      "@material-ui/core": "=5.0.0-alpha.34",
      "@material-ui/icons": "=5.0.0-alpha.34",
      "@material-ui/lab": "=5.0.0-alpha.34",
      "@material-ui/styles": "=5.0.0-alpha.33",
      "@material-ui/utils": "=5.0.0-alpha.33",
      "apollo-upload-client": "^16.0.0",
      "change-case": "^4.1.2",
      "class-transformer": "^0.4.0",
      "class-validator": "^0.13.1",
      "date-fns": "^2.22.1",
      "deepmerge": "^4.2.2",
      "faker": "^5.5.3",
      "framer-motion": "^4.1.17",
      "graphql": "^15.5.0",
      "jsonwebtoken": "^8.5.1",
      "jss": "^10.6.0",
      "jss-rtl": "^0.3.0",
      "lodash": "^4.17.21",
      "next": "^10.2.3",
      "notistack": "1.0.9",
      "nprogress": "^0.2.0",
      "react": "17.0.2",
      "react-dom": "17.0.2",
      "react-hook-form": "^7.8.4",
      "react-intersection-observer": "^8.32.0",
      "react-scroll": "^1.8.2",
      "simplebar": "^5.3.3",
      "simplebar-react": "^2.3.3",
      "stylis": "^4.0.10",
      "stylis-plugin-rtl": "^2.1.0"
   },
   "devDependencies": {
      "@babel/core": "^7.14.5",
      "@babel/plugin-proposal-decorators": "^7.14.5",
      "@types/apollo-upload-client": "^14.1.0",
      "@types/graphql": "^14.5.0",
      "@types/lodash": "^4.14.170",
      "@types/react": "^17.0.11",
      "@typescript-eslint/eslint-plugin": "^4.26.1",
      "@typescript-eslint/parser": "^4.26.1",
      "apollo": "^2.33.4",
      "eslint": "^7.28.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-import": "^2.23.4",
      "eslint-plugin-jsx-a11y": "^6.4.1",
      "eslint-plugin-prettier": "^3.4.0",
      "eslint-plugin-react": "^7.24.0",
      "eslint-plugin-react-hooks": "^4.2.0",
      "eslint-plugin-simple-import-sort": "^7.0.0",
      "eslint-plugin-unused-imports": "^1.1.1",
      "npm-check-updates": "^11.6.0",
      "prettier": "^2.3.1",
      "typescript": "^4.3.2"
   }
}
